#include <iostream>
using namespace std;
#include "conio.h"
#include "math.h"
int main ()
{
double pr=1;
int n;
cout<<"Ingrese el ultimo valor: "<< endl;
cin>>n;
for (int i=1;i<=n;i++)
{
    if (i%2==0) {
    pr = pr * i ;
    }

}
cout<<"La multiplicacion de los pares hasta: "<<n<<" es:"<< pr <<endl;
    getch ();
}
